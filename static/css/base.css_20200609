@charset "utf-8";

/*----------------------------------------------------------------------
    0.Body 設定
----------------------------------------------------------------------*/
body {
    text-align: center;
    color:#333333;
    margin: 0px;
    padding: 0px;
    font-family: "メイリオ", Meiryo, "游ゴシック体", YuGothic, "游ゴシック", "Yu Gothic", "ヒラギノ角ゴ Pro W3", "Hiragino Kaku Gothic Pro", Osaka, "ＭＳ Ｐゴシック", "MS PGothic", sans-serif;
    font-size: 13px;
    line-height: 1.0;
    background-image: url(../image/bg-body.png);
}
body.bg-dark {
    background-image: url(../image/bg-body-dark.png);
    background-color: #eee;
}
a{
    color: #2D699F;
    text-decoration: none;
}
a:hover{
    color: #1C8EBF;
    text-decoration: underline;
}
a[href^="tel:"] {
    pointer-events: none;
}
img {
    vertical-align: bottom;
}

.sticky {
    position: -webkit-sticky;
    position: sticky;
}

/*----------------------------------------------------------------------
    1.書体
----------------------------------------------------------------------*/
/* サイズ */
.txt9 {font-size: 9px; line-height: 1.22;}
.txt10 {font-size: 10px; line-height: 1.2;}
.txt10h {font-size: 10px; line-height: 1.5;}
.txt12 {font-size: 14px; line-height: 1.25;}
.txt12h {font-size: 14px; line-height: 1.5;}
.txt14 {font-size: 15px; line-height: 1.25;}
.txt14h {font-size: 15px; line-height: 1.5;}
.txt16 {font-size: 16px; line-height: 1.19;}
.txt18 {font-size: 18px; line-height: 1.1;}
.txt18h {
    font-size: 18px;
    line-height: 1.3em;
    font-weight: bold;
    color: #FF0000;
}
/* テキスト位置 */
.rightt{
    text-align: right;
}
.leftt{
}
/* マージン */
.under10{margin-bottom: 10px;}
.under15{margin-bottom: 15px;}
.under20{margin-bottom: 20px;}
.under25{margin-bottom: 25px;}
.under30{margin-bottom: 30px;}
.under40{margin-bottom: 40px;}
/* 色 */
.clred {color:#FF0000;}
.clcrimson {color:#CC0000;}
.clblue {color:#0000FF;}
.clsea {color:#006699;}
.clblack {color:#000000;}
.cllightred {color:#ef5350;}
.cldarkgray {color:#333333;}
.clgray {color:#666666;}
.cldarkorange {color:#f57c00;}
.cldarkblue {color:#00467e;}
.cllightblue {color:#00a3d9;}

/* 装飾 */
.heavy {font-weight:bold;}
/*見出しタグ*/
h2.brown,
h2.blue,
h2.yellow,
h2.green,
h2.gray,
h2.yellow_l2,
h2.green_l2 {
    font-size: 16px;
    font-weight: bold;
    display: block;
    height: auto;
    width: 500px;
    line-height: 1.4;
    margin: 25px 0px 15px;
    padding: 8px 10px 8px 20px;
}
h2.brown {
    background: url(../image/bg_common_h2-brown.png) repeat-y;
    color: #663300;
}
h2.blue {
    background: url(../image/bg_common_h2-blue.png) repeat-y;
    color: #00838f;
}
h2.yellow {
    background: url(../image/bg_common_h2-yellow.png) repeat-y;
    color: #855A12;
}
h2.green {
    background: url(../image/bg_common_h2-green.png) repeat-y;
    color: #558b2f;
}
h2.gray {
    background: url(../image/bg_common_h2-gray.png) repeat-y;
    color: #666666;
}
h2.yellow_l2 {
    background: url(../image/bg_common_h2-yellow.png) repeat-y;
    color: #855A12;
}
h2.green_l2 {
    background: url(../image/bg_common_h2-green.png) repeat-y;
    color: #558b2f;
}

.no-area-pr-fist-item > h2.brown:first-child,
.no-area-pr-fist-item > h2.blue:first-child,
.no-area-pr-fist-item > h2.yellow:first-child,
.no-area-pr-fist-item > h2.green:first-child,
.no-area-pr-fist-item > h2.gray:first-child,
.no-area-pr-fist-item > h2.yellow_l2:first-child,
.no-area-pr-fist-item > h2.green_l2:first-child {
    margin-top: 0;
}

h2.has_parenthes{
    padding-left: 15px;
}
h2.brown.no-margin-top,
h2.blue.no-margin-top,
h2.yellow.no-margin-top,
h2.green.no-margin-top,
h2.gray.no-margin-top{
    margin-top: 0;
}

/*----------------------------------------------------------------------
    2.左右カラム構造
----------------------------------------------------------------------*/
#wrapper {
    background-image: url(../image/bg-wrapper.png);
    text-align: left;
    margin: 0 auto 30px;
    padding: 10px;
    width: 760px;
}
#left {
    float: left;
    width: 195px;
}
#right {
    margin-left: 210px;
    padding: 0px;
}


/*----------------------------------------------------------------------
    3.ヘッダ
----------------------------------------------------------------------*/
/* ヘッダレイアウト */
#header {
    background-image: url(../image/header-bg.png);
    background-size: cover;
    background-color: #fff;
    text-align: left;
    clear: both;
    width: 100%;
}
#top-header {
    overflow: hidden;
    position: absolute;
    top: 0;
    width: 100%;
    z-index: 10;
}
#header .header-inner {
    margin: 0px auto;
    width: 760px;
    height: 84px;
}
#top-header .header-inner {
    margin: 0px auto;
    width: 90%;
}
#header-info {
    margin-left: 200px;
}

/* サイトロゴ */
#header-logo {
    float: left;
    width: 180px;
    height: 52px;
    background: url(../image/head_logo_white-180.png) no-repeat 0% 0%;
    background-size: 180px 52px;
    clear: both;
    margin-top: 15px;
}

#top-header #header-logo {
    background: url(../image/head_logo_white-180.png) no-repeat 0% 0%;
    background-size: 180px 52px;
    margin-top: 30px;
    width: 180px;
    height: 52px;
}

#header-logo a {
    display: block;
    height: 47px;
    width: 160px;
}
#header-logo a span {display:none;}

/* ISMSバナー */
#header-info div.isms {
    float:right;
    height: 38px;
    width: 190px;
    margin-top: 5px;
    margin-bottom: 5px;
    clear:right
}
/* 企業サイトと英中文サイトへのリンク */
#header-info div.link {
    float:right;
    width: 350px;
    margin-right:5px;
    height: 48px;
}
#header-info .link a {
    text-align:center;
    border-top: 0px none;
    border-right: 1px solid #CCCCCC;
    border-bottom: 1px solid #CCCCCC;
    border-left: 1px solid #CCCCCC;
    text-decoration: none;
    font-size: 12px;
    line-height: 2.0;
    margin-right: 6px;
    float: right;
    /* background: #F5F5F5; */
    /* color: #333333; */
    background: inherit;
    color: #ddd;
}
#top-header #header-info .link a {
    background: inherit;
    color: #ddd;
}
#header-info .link a.sbjp {
    display: block;
    width: 155px;
    height: 24px;
}
#header-info .link a.ench {
    display: block;
    width: 75px;
    height: 24px;
}
/* #header-info .link a:hover {color: #000000; text-decoration:none; background: #EEEEEE;} */
#header-info .link a:visited {}
#header-info .link a:active {}

#header #header-info .sp-btn-block,
#top-header #header-info .sp-btn-block{
    display: none;
}

/* header menu */
ul#header-menu {
    list-style:none;
    float: right;
    display: inline-block;
    clear: right;
    width: 553px;
}
ul#header-menu li {
    list-style:none;
    float:left;
}
ul#header-menu a {
    display:block;
    height:22px;
}
#header-menu a span {display:none;}
#header-menu a.top {
    width:81px;
    background: url(../image/head_menu_top.jpg);
}
#header-menu a:hover.top {
    background-position:0px -22px;
}
#header-menu a.company {
    width:56px;
    background: url(../image/head_menu_company.jpg);
}
#header-menu a:hover.company {
    background-position:0px -22px;
}
#header-menu a.greeting {
    width:66px;
    background: url(../image/head_menu_greeting.jpg);
}
#header-menu a:hover.greeting {
    background-position:0px -22px;
}
#header-menu a.iso {
    width:71px;
    background: url(../image/head_menu_iso.jpg);
}
#header-menu a:hover.iso {
    background-position:0px -22px;
}
#header-menu a.link {
    width:82px;
    background: url(../image/head_menu_link.jpg);
}
#header-menu a:hover.link {
    background-position:0px -22px;
}
#header-menu a.sitemap {
    width:73px;
    background: url(../image/head_menu_sitemap.jpg);
}
#header-menu a:hover.sitemap {
    background-position:0px -22px;
}
#header-menu a.privacy {
    width:124px;
    background: url(../image/head_menu_privacy.jpg);
}
#header-menu a:hover.privacy {
    background-position:0px -22px;
}

/*----------------------------------------------------------------------
    TOP動画
----------------------------------------------------------------------*/
#video-bg{
    position: relative;
    overflow: hidden;
    background-image: url(../image/top/poster-2.png);
    background-size: contain;
    background-repeat: no-repeat;
    background-position: center;
    background-color: #444;
    width: 100vw !important;
    height: 56.3vw !important;
    /* max-height: 850px; */
    max-height: 800px;
}

#video-bg.video-bg-1{
    background-image: url(../image/top/poster-top-1.png);
}

#video-bg.video-bg-2{
    background-image: url(../image/top/poster-top-2.png);
}

#video-bg.video-bg-3{
    background-image: url(../image/top/poster-top-3.png);
}

#video-bg.mv-bg-1{
    background-image: url(../image/top/mv-bg-1.jpg);
    background-size: cover;
    height: 56.3vw !important;
}

#video-bg .bx-wrapper{
    display: none;
}

#video-bg .video{
    position: absolute;
    width: 100%;
    height: 100%;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
}

#video-bg .video.video-m{
    display: none !important;
}

#video-bg .mv-text-block{
    position: absolute;
    top: 9vw;
    left: 0;
    right: 0;
    color: #fff;
    animation: mv-color 20s infinite;
    margin: 0 auto;
    text-align: center;
    text-shadow: 1px 1px 1px #00467e;
}

#video-bg .mv-link-wrap{
    position: absolute;
    top: 300px;
    top: 39vw;
    left: 0;
    right: 0;
    color: #fff;
    animation: mv-color 20s infinite;
    margin: 0 auto;
    text-align: center;
    text-shadow: 1px 1px 1px #00467e;
}
#video-bg .mv-title{
    display: inline-block;
    padding-top: 30px;
    padding-top: 3vw;
    padding-bottom: 26px;
    padding-bottom: 2vw;
    font-size: 52px;
    font-size: 4vw;
    font-weight: bold;
    text-align: left;
}
#video-bg .mv-title .s{
    display: block;
    margin-bottom: 3px;
    font-size: 14px;
}
#video-bg .mv-title .b{
    font-size: 53px;
    font-size: 4.2vw;
}
#video-bg .mv-copy{
    font-size: 17px;
    font-size: 2.2vw;
    line-height: 1.7;
    font-weight: bold;
}
#video-bg .mv-link{
    display: inline-block;
    box-sizing: content-box;
    background-image: url(../image/bg-light-blue-opacity60.png);
    border: 1px solid #fff;
    margin: 0 0.8em;
    padding: 0 24px;
    height: 44px;
    line-height: 44px;
    color: #fff;
    font-size: 18px;
    font-weight: bold;
    text-shadow: none;
}
#video-bg .mv-link:hover{
    text-decoration: none;
    background-image: none;
    background-color: #22b5e5;
}

#video-bg .mv-link.hover-red:hover{
    background-color: #f44336;
}

#video-bg .mv-link::after{
    content: url(../image/mv-link-arrow-white.png);
    position: relative;
    top: 1px;
    left: 2px;
}
#video-bg .mv-link:first-child{
    margin-left: 0;
}
#video-bg .mv-link:last-child{
    margin-right: 0;
}
.video-yt-wrap{
    position: relative;
    margin: 0 auto;
    max-width: 1440px;
}
.video-yt-wrap::before{
    content: "";
    display: block;
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 1;
}

/*----------------------------------------------------------------------
    4.グローバルメニュー（中ページ共通）
----------------------------------------------------------------------*/

#global {
    top: 0px;
    background-color: #00a3d9;
    /* background-image: url(../image/bg-global.png); */
    background-position: left bottom;
    clear:both;
    margin-right: 0px;
    margin-left: 0px;
    z-index: 10;
}

#video-bg #global {
    position: absolute;
    left: 0;
    right: 0;
    top: auto;
    bottom: 0;
    background-color: transparent;
    background-image: url(../image/bg-dark-gray-opacity60.png);
}

#global ul {
    list-style: none;
    display: block;
    margin-right: auto;
    margin-left: auto;
    clear: none;
    width: 760px;
    height: 46px;
    overflow: hidden;
}

#video-bg #global ul {
    height: 56px;
}

#global li {
    list-style:none;
    float:left;
}

#global a {
    display: block;
    background-image: url(../image/bg-white-opacity30.png);
    margin-top: 5px;
    margin-bottom: 6px;
    margin-left: 1px;
    margin-right: 1px;
    padding-top: 12px;
    width: 124px;
    height: 24px;
    color: #fff;
    line-height: 1;
}

#video-bg #global a {
    padding-top: 17px;
    height: 29px;
}

#global a:hover {
    background-image: none;
    background-color: #78d2f0;
}

#video-bg #global a:hover {
    background-color: #62bbd9;
}

#global a span {
    display: inline-block;
}
#global a span::before {
    position: relative;
    top: -2px;
    content:url(../image/gnav_arrow.png);
    display: inline-block;
    margin-right: 4px;
}
#global a:hover span::before{
    opacity: 0.8;
}


