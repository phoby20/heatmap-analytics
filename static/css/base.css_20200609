/*----------------------------------------------------------------------
    5.フッター
----------------------------------------------------------------------*/

/* フッターレイアウト */
#footer {
    margin: 0;
    padding-bottom: 15px;
    background-color: #EEEEEE;
    clear:both;
}
#footer-wrapper {
    text-align: left;
    margin: 0px auto;
    width: 760px;
}
#footer .footer-line {
    /* background: url(../image/line_footer_02.gif) repeat-x 50% 50%; */
    clear: both;
    padding-top: 15px;
    padding-bottom: 3px;
}
/* フッターメニュー */
#footer-wrapper .menu {
    font-size: 12px;
    line-height: 2;
    padding-top: 10px;
}
#footer-wrapper .menu img {
    padding-top: 12px;
    padding-bottom: 3px;
    display: block;
    clear: left;
}
#footer-wrapper .menu ul {
    clear: both;
    display: block;
}
#footer-wrapper .menu li {
    display: block;
    float: left;
    margin-right: 12px;
    padding-left: 16px;
    background: url(../image/icon_footermenu_gray_01.gif) no-repeat 0% 50%;
    white-space: nowrap;
    }
/* インフォメーション & コピーライト */
#footer-wrapper .info {
    font-size: 10px;
    line-height: 1.1;
}
#footer-wrapper .info img {
    padding-top: 5px;
    padding-bottom: 10px;
    display: block;
    clear: left;
}
/* リンク文字周りの設定 */
#footer-wrapper a {color:#377193; text-decoration: none;}
#footer-wrapper a:hover {color:#0099CC; text-decoration: underline;}
#footer-wrapper a:visited {}
#footer-wrapper a:active {}

#footer-wrapper a[target="_blank"]{
    background-image: url(../image/icon-window-mini.png);
    background-size: 18px 15px;
    background-position: right center;
    background-repeat: no-repeat;
    padding-right: 23px;
}

